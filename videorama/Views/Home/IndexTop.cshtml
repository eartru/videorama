@model videorama.ViewModels.AccueilViewModel

@foreach (var item in Model.Product)
{
    <tr>
        <td>
            <a href=@Url.Action("Details", "Products", new { id = item.IdProduct }, null)>
                @Html.DisplayFor(modelItem => item.Title) 
            </a>
        </td>
    </tr>
}
